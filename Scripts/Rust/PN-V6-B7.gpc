function data_extract(f_string,f_idx,f_location){

return Screen1_Data[(f_idx / 2) + data_offset(f_string)] >> (((f_idx & 1) * 16)+ ((f_location & 1)*2)) & (((f_location & 1) * 0x3FFC)+ 0x3);
}

function data_offset(f_string){

	return d_size[f_string]
}

function char_offset(f_string){

	return Screen1[s_size[f_string] + z] 
}

function PRINT(f_string,f_idx,f_x,f_y,f_font,f_color,f_type){
	if(!f_type){
		d = data_extract(f_string,f_idx,0)
		z = data_extract(f_string,f_idx,1)		
		for(a = 1; a <= size(f_string,f_idx); a++, d++){
			if(d == 4){
				d = 0
				z++
			}
			putc_oled(a,char_offset(f_string) >> (0x8 * d) & 0xFF);			
		}
		
		
		puts_oled(x_location(size(f_string,f_idx),f_font,f_x),f_y,f_font,size(f_string,f_idx),f_color);
	
	
	}
    else if (f_type == 1){
        if(f_string < 0){putc_oled(1,45);}
        if(f_string){
	        for(n = find_digits(f_string) + z = (f_string < 0 ),f_string = abs(f_string); f_string > 0; n-- , z++){  
	            putc_oled(n,f_string%10 + 48);
	            f_string /= 10
	        }
        }
        else{
        	putc_oled(1,48); z = 1
        }
        puts_oled(x_location(z,f_font,f_x),f_y,f_font,z ,f_color);
    }

}

function SS (){
	timer += get_rtime()	
	switch(ss_step){
		case 0:{
			ss_reset()
			center_start = x_location(S[0] + S[1] + S[2] + S[3],OLED_FONT_SMALL,-2)
