function convert (f_val){
    return (f_val * 32767) / 100
}

function Toggle_Bit(v,i){ return (v = (v ^ (1 << i)));}
function Test_Bit(v,i){ return (v & ( 1 << i))}

function SmartSens(f_val, f_axis) { 

    SmartSens = ( (f_val * ( 32767 - abs( get_val(f_axis) ) ) ) / 32767); 

    return clamp(get_val(f_axis) + SmartSens, -32768, 32767);
}

function unpack (f_val,f_type){
	if(!f_type){
		if(f_val & 0x8000){
			return (~(f_val & 0x7FFF)) + 1
		}
		else{
			return f_val & 0x7FFF
		}
	}

	return (f_val >> 16) & 0xFFFF 

}

