		((data_extract(f_string,f_idx + 1,1) * 4) + (data_extract(f_string,f_idx + 1,0) - 1)) - ((data_extract(f_string,f_idx,1) * 4) + data_extract(f_string,f_idx,0) )   		
	
}

function find_data(f_string){
	A = 0;
	Find = FALSE;
	for(z = 0; z < string_size [f_string] ;z++){
		for (a = 0 ; a < 4; a++){
			if(Find){		
				for(I = 0; I < 2; I++){
					Screen1_Data[(A  >> 1) + data_offset(f_string)] = (a[I] << (((A & 1) * 16) + (I * 2))) | Screen1_Data[(A >> 1) + data_offset(f_string)]
				}
				A++
				Find = FALSE
			}
			if((char_offset(f_string) >> (0x8 * a) & 0xFF) == 0){
				Find = TRUE;			
			}
		}
	}
}


function x_location(f_chars,f_font,f_x) {
    if(f_x == -5){return 96 - ((f_chars * (7 + (f_font > 1) + f_font * 4)) >> 1)}
    if(f_x == -4){return 32 - ((f_chars * (7 + (f_font > 1) + f_font * 4)) >> 1)}
    if(f_x == -3){return 128 - ((f_chars * (7 + (f_font > 1) + f_font * 4)) + 3 );}
    if(f_x == -2){return 64 - ((f_chars * (7 + (f_font > 1) + f_font * 4)) >> 1);}    
    if(f_x == -1){return 3}
    return f_x;     
}




function find_digits(f_num) {
    for(n = 1; n < 11;n++){
        if(!(abs(f_num) / pow(10,n))){
               return n; 
               break;
          }
    }
    return 1;
}

