function cycle(f_val, f_lo, f_hi) {
	if(f_val > f_hi) return f_lo;
	if(f_val < f_lo) return f_hi;
	return f_val;
}




function edit_val(f_val,f_inc,f_min,f_max,f_bdec,f_binc,f_kdec,f_kinc,f_time){
	if(press_hold(f_bdec,f_kdec,f_time)){
		f_val = cycle( f_val - f_inc,f_min,f_max)		
		Display = TRUE;
	}
	if(press_hold(f_binc,f_kinc,f_time)){
		f_val = cycle( f_val + f_inc,f_min,f_max)
		Display = TRUE;
	}
	return f_val;
}


function press_hold(f_button,f_key,f_time){
    if((get_ival(f_button) && get_ptime(f_button) > 500 ) || (get_key(f_key) && key_ptime(f_key) > 500 )  ){
        if(((get_ptime(f_button)%(f_time + get_rtime())) >= f_time) || ((key_ptime(f_key) %(f_time + get_rtime())) >= f_time )){
            return TRUE        
        }    

    }

    return event_press(f_button) || key_press(f_key)
}





