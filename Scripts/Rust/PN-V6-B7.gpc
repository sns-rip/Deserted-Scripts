int active_key, release_key, key_buffer, key_list_idx,  _continue,  k_timer;
int key_array[6];
int key_array_ptime[6];

function key_press(a) { return (a == active_key); }
function key_release(a) { return (a == release_key); }
function key_ptime(a) {
  if (get_key(a))
  for (z = 0; z <= 6; z++)
  {
    if (a == key_array[z]) return  (k_timer - key_array_ptime[z]) + get_rtime();
  }
  return 0;
}


function key_events(temp) {
   k_timer += get_rtime();
  active_key = release_key = 0;
  if (key_buffer) {
    for (z = 0; z < 6; z++){
      if (key_array[z] && !get_key(key_array[z])) { release_key = key_array[z]; temp = z; break; }
    }
    if (release_key) {
      key_array[temp] = 0;
      key_array_ptime[temp] = 0;
      key_buffer--;
    }
  }
  if (key_buffer < 6) {
    for (temp = 0; temp < (sizeof(key_list) / 4); temp++) {
      _continue = 1;
      if (get_key(key_list[temp])) {
        for (z = 0; z < 6; z++) {
          if (key_array[z] == key_list[temp]) { _continue = 0; break; }
        }
        if (_continue) {
          for (z = 0; z < 6; z++) {
            if (!key_array[z]) {
              key_array[z] = key_list[temp];
              
              break;
            }
          }
          key_array_ptime[z] =  k_timer;
          active_key = key_list[temp];
          key_buffer++;
        }
      }
    }
  }
}
function get_key(key) {
  if (key >= 0xE0) {
    return get_modifiers(1 << (key ^ 0xE0));
  }
  return get_keyboard(key);
}

