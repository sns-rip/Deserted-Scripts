int red = 255, green = 0, blue = 0;
int colorStep = 5;

combo RLED {
    if (red > 0 && blue == 0) {
        red -= colorStep;
        green += colorStep;
    }
    if (green > 0 && red == 0) {
        green -= colorStep;
        blue += colorStep;
    }
    if (blue > 0 && green == 0) {
        blue -= colorStep;
        red += colorStep;
    }
    set_rgb(red, green, blue);
    
    wait(20);
}

main {
    combo_run(RLED);
