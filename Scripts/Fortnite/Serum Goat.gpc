function V_060(V_061) {                         
//  find_digits(value)                                
//        return Number of Digits in Value Passed

    V_061 = abs(V_061);                               
    if(V_061 / 0x2710 > 0x0) return 0x5;                   
    if(V_061 /  0x3E8 > 0x0) return 0x4;                   
    if(V_061 /   0x64 > 0x0) return 0x3;                   
    if(V_061 /    0xA > 0x0) return 0x2;                   
                          return 0x1;                  
}                                                     
