// Variables for function number_to_string()  
int V_0360; 
int V_0361,V_0362; 
function V_047(V_042,V_049) {                  
//=======================================================
//  number_to_string(value to convert, # of digits in value) 
//                                                           
//  Display Up to Max 16-bit Signed Integer (-32,768 to 32,767) 
//                                                      
//      Convert digits to ASCII                         
//      Add ASCII character to print buffer            
//      Print string centered X & Y on OLED             
//                                                       
//======================================================
    V_0360 = 0x1;  V_0362 = 0x2710;

    if(V_042 < 0x0) //--neg numbers                     
    {                                                 
         putc_oled(V_0360,0x2D);    //--add leading "-"      
         V_0360 += 0x1;                                      
         V_042 = abs(V_042);                          
    }

    for(V_0361 = 0x5; V_0361 >= 0x1; V_0361--)                           
    {                                                 
        if(V_049 >= V_0361)                             
        {                                             
            putc_oled(V_0360,V_0364[V_042 / V_0362]);    
            V_042 = V_042 % V_0362;                    
            V_0360 +=  0x1;                                  
            if(V_0361 == 0x4)                               
            {                                         
                putc_oled(V_0360,0x2C);   //--add ","        
                V_0360 += 0x1;                               
            }                                        
        }                                             
        V_0362 /= 0xA;                                  
    }                                                
    puts_oled(V_062(V_0360 - 0x1,OLED_FONT_MEDIUM_WIDTH),0x1B,OLED_FONT_MEDIUM,V_0360 - 0x1,OLED_WHITE); // adjustable value centered in X 
}                                                     
