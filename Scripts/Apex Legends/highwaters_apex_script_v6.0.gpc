function set_weapon(button, number, btn_count, t) {
	if(get_ival(button))
		set_val(button, 0);
	if(btn_count >= 1) {
		if(t == _wpn) {
			just_selected = TRUE;
			to_set = btn_count - 1;
			in_hand = btn_count - 1;
			weapon[to_set] = number;
		}
		if(t == _brrl && weapon[to_set] != 0) {
			to_set = btn_count - 1;
			barrel[to_set] = number;
			if(barrel[to_set] > 3 || GUN_INFO[weapon[to_set]][2] > 1)
				barrel[to_set] = 0;
			color = barrel[to_set] + 3;
		}
		if(t == _site && weapon[to_set] != 0) {
			to_set = btn_count - 1;
			sight[to_set] = number;
			if(sight[to_set] > 2 || (GUN_INFO[weapon[to_set]][2] && GUN_INFO[weapon[to_set]][2] <= 2 && sight[to_set] > 1) || GUN_INFO[weapon[to_set]][2] == 4)
				sight[to_set] = 0;
			color = sight[to_set] + 4;
		}
		if(t == 30) {
			in_hand = btn_count - 1;
			weapon[in_hand] = number;
			barrel[in_hand] = number;
			sight[in_hand]  = number;
			oled = _in_hand;
		}
		if(t == 30 && button == PS4_TOUCH) {
			in_hand = 0;
			weapon[0] = number;
			barrel[0] = number;
			sight[0]  = number;
			weapon[1] = number;
			barrel[1] = number;
			sight[1]  = number;
			oled = _in_hand;
		}
		if(t && t != 30)
			oled = t;
		double_tap_count[button] = 0;
	}
}
