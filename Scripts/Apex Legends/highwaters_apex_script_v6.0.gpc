			return TRUE;
		if(on == 2 && get_ival(FIRE) && !get_ival(ADS))
			return TRUE;
	}
	if((ping && type == 1) || (type == 2 && get_ival(PressBtns[ACTIVATE])) || (type == 3 && keep[time_shot] > 0 && get_ptime(FIRE) < 300)) {
		if(on == 0 && get_ival(FIRE))
