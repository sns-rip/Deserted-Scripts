					//set_val(PING,100);
				block_input(PING);
			}
			if(horizonX)
			{
				block_input(ADS);
				horizonX = FALSE;
			}
			if(get_ival(PING) && event_press(TACTICAL)){
				in_hand = 2;
				weapon[in_hand] = snipers_mark;
				holster = FALSE;
				ultimate = TRUE;
				oled = _in_hand;
				color = 6;
			}
			break;
		}
     // Rampart
		case rampart: {// Easy Rampart Shield
			if(get_ptime(TACTICAL) > 450 && event_release(TACTICAL)){
				combo_run(R_SHIELD_CMB);
			}
			if(get_ival(PING) && event_press(TACTICAL)){
				in_hand = 2;
				weapon[in_hand] = sheila;
				ultimate = TRUE;
				oled = _in_hand;
				color = 6;
			}
			break;
		}
	// crypto
        case crypto: {
