				if(get_ival(PING) && get_ptime(PING) <= get_ptime(ADS) && get_ival(ADS)) {	        //╠═╝╠═╣║║║║ ╦  │  ╚═╗║║║║║ ╦║  ║╣   ╠╩╗║║  ║    ╚═╗╠═╣║ ║ ║ 															       
																									//╩  ╩ ╩╝╚╝╚═╝  o  ╚═╝╩╝╚╝╚═╝╩═╝╚═╝  ╩ ╩╩╩═╝╩═╝  ╚═╝╩ ╩╚═╝ ╩ 
						pang = TRUE;
						oled = _png;
				}
				if(pang) {																			//┌┐ ┬ ┬  ╔═╗┬─┐┌─┐┌─┐┌─┐┌─┐┌┐┌┌─┐                           
					if(get_ival(PING))																//├┴┐└┬┘  ║  ├┬┘├┤ └─┐│  ├┤ │││└─┐                           
						combo_run(BANGER);												    		//└─┘ ┴   ╚═╝┴└─└─┘└─┘└─┘└─┘┘└┘└─┘   
					block(PING, get_ptime(PING) + 50);
				}
