enum {no_legend,crypto,horizon,revenant,rampart,wattson,valkyrie,wraith,bangalore,newcastle,gibraltar,lifeline,seer,bloodhound,pathfinder,mirage,fuse,caustic,ash,octane,loba,mad_maggie,vantage,catalyst}
function legend_mods() {
// Legend Mods
	legend_check = not_selecting && !get_ival(RELOAD) && !get_ival(ADS) && !get_ival(JUMP);
	switch(legend) {
	// Pathfinder
        case pathfinder: {
			if(get_ival(TACTICAL) && get_rumble(RUMBLE_A) == 100 && get_rumble(RUMBLE_B) == 100) // better grapple
				set_val(JUMP,100);
        	if(get_ival(PressBtns[LEGEND]) && get_ptime(PressBtns[LEGEND]) > 150 && legend_check) // Pathfinder's Slide Grapple
        	    combo_run(P_SLIDE_G);
        	else
        	    combo_stop(P_SLIDE_G);
        	if(get_ival(PressBtns[LEGEND]) && get_brtime(PressBtns[LEGEND]) < 150 && legend_check)  // Pathfinder's Grapple Hop
        	    combo_run(GH_CMB);
        	break;
		}
	// Newcastle
		case newcastle: {
			if(event_press(PressBtns[LEGEND]) && legend_check)  // Newcastle Quiet Revive
				combo_run(NC_QUIET_REZ_CMB); 
			if(combo_running(NC_QUIET_REZ_CMB)) {
				set_val(MOVE_Y,0);
				set_val(MOVE_X,0);
			}
			break;
		}	
	// Wattson
		case wattson: {// Easy Wattson Fence	
			if(get_ptime(TACTICAL) > 200 && event_release(TACTICAL))
				combo_run(W_FENCE_CMB);
			break;
		}
	// Valkyrie
		case valkyrie: {// Valkyrie Behind Missile Swarm
			if(event_press(PressBtns[LEGEND]) && legend_check)
				combo_run(MSWARM_CMB);
			if(event_press(JUMP) && get_brtime(JUMP) <= 250){ // Valkyrie Strafe Mod
    			jetpack = TRUE;
    			ultimate = 1;
    		}
    		if(jetpack && event_press(JUMP))
    		{ 
        		//combo_stop(VALK_STRAFE_CMB);
        		ultimate = ultimate * -1;
        		combo_run(VALK_STRAFE_CMB);
    		}
    		if(jetpack && (event_press(CROUCH) || get_ptime(JUMP) > 5000))
        		jetpack = FALSE;
			break;
			}
	// Revenant
		case revenant: {// Revenant's Silence + Nade Combo
			if(get_ival(PressBtns[LEGEND]) && legend_check){
				set_val(TACTICAL,100);
				combo_run(REV_P1);
				combo_run(REV_P2);
			} else {
				combo_stop(REV_P1);
				combo_stop(REV_P2);
			}
			break;
		}
	// Wraith
		case wraith: {
			if(!ultimate && get_ival(PressBtns[LEGEND]) && get_ptime(PressBtns[LEGEND]) > 250 && legend_check)  // Wraith Ult + Tactical
            	combo_run(WRAITH_UT);  
            if(get_ival(PING) && event_press(TACTICAL))
                ultimate = !ultimate;
            if(ultimate && event_press(PressBtns[LEGEND]) && legend_check) // Wraith Crouch Portal
                combo_run(C_PORTAL_CMB);
            break;
		}
	// Banglore
        case bangalore: {// bangalore smoke and heal 
        	if(get_ival(PressBtns[LEGEND]) && get_ptime(PressBtns[LEGEND]) > 150 && legend_check) // bangalore slide jump ult
            	combo_run(cBANG_SJ_HS);
            if(combo_running(cBANG_SJ_HS))
                combo_stop(cBANG_SJ_S)
            if(get_ival(PressBtns[LEGEND]) && get_ptime(PressBtns[LEGEND]) < 150 && legend_check)  // bangalore slide smoke jump smoke
                combo_run(cBANG_SJ_S);
            if(combo_running(cBANG_SJ_S))
                combo_stop(cBANG_SJ_HS);  
            break;
        }
	// Bloodhound 
		case bloodhound: {// Bloodhound Ult Scan	
			if(event_press(PressBtns[LEGEND]) && legend_check)
				combo_run(BLOODHOUND_US_CMB);
			break;
		}
	// Lifeline 
		case lifeline: {	
			if(get_ival(PS4_LY) >= -84 && event_press(PressBtns[LEGEND]) && legend_check) // Lifeline Revive + Healing Drone
				combo_run(LIFELINE_RD);
			if(get_ival(PS4_LY) <= -85 && event_press(PressBtns[LEGEND]) && legend_check) // Lifeline Slide Revive
				combo_run(LL_SR_CMB);
			break;
		}
	// Caustic
		case caustic: {// Caustic Panic Ult	
			if(event_press(PressBtns[LEGEND]) && legend_check)
				combo_run(C_PU_CMB);
			break;
		}
		// Fuse 
		case fuse: {
			if(legend_check) {
    			if(get_ival(PressBtns[LEGEND]) && (get_ptime(PressBtns[LEGEND]) >= 200)) {
					set_val(GRENADE, 100 * !(get_ptime(PressBtns[LEGEND]) >= 300));
					grenade = TRUE;
 					combo_run(FUSE_NS);
    			}
    			if(event_release(PressBtns[LEGEND]))
    				combo_run(F_PU_CMB);
  			}
  			if(event_release(PressBtns[LEGEND])){
    			combo_stop(FUSE_NS);
    			combo_run(FUSE_NS_SWAP);
  			}
  			break;
		}
	// Horizon 
		case horizon: {
			if(get_ival(PressBtns[LEGEND]) && get_ptime(PressBtns[LEGEND]) >= 200 && legend_check){ // Horizon Ult & Nade
				set_val(TACTICAL,100);
				set_val(PING,100);
				ultimate = TRUE;
			}
			if(event_release(PressBtns[LEGEND]) && ultimate && legend_check){
				combo_run(H_UN_CMB);
			}
			if(event_release(PressBtns[LEGEND]) && !ultimate && legend_check) // Horizon lift & heal
    			combo_run(HORIZON_TH_CMB);
   		    if(horizonX && left_stick_radius > 90)
       			horizonX = FALSE;
       		if(horizonX)
       			combo_run(HORIZON_LR_CMB);
   		    if((get_ival(FIRE) && left_stick_radius > 75 && get_val(CROUCH)) || combo_running(BUNNY_HOP)) {// moonwalk shot
				set_val(CROUCH,100);
				combo_run(BUNNY_HOP);
   			}
   		    if(event_release(FIRE))
   				combo_stop(BUNNY_HOP);
			break;
		}
	// Octane
		case octane: {// Octane Heal & Stim
			if(event_press(PressBtns[LEGEND]) && legend_check)
				combo_run(OCTANE_SS_CMB);
			break;
		}
	// Seer
		case seer: {// Seer Passive Spam
			if(get_ival(RELOAD) && get_ptime(RELOAD) > 350 && not_selecting &&  !get_ival(ADS) && !get_ival(JUMP))
				combo_run(SEER_P_S_CMB);
			break;
		}
	// Mad Maggie
		case mad_maggie: {// Mad Maggie Ult & Tactical
			if(event_press(PressBtns[LEGEND]) && legend_check)
				combo_run(MM_UT_CMB);
			break;
		}
	// Ash
        case ash: {// Ash Quick Ult    
        	/*if(get_ival(PressBtns[LEGEND]) && legend_check) // Ash Quick Ult + Tactical
            	combo_run(ASH_QUT_CMB);
            if(event_release(PressBtns[LEGEND])){
                combo_run(ASH_T_CMB);
                combo_stop(ASH_QUT_CMB);
            }*/   
            if(event_release(PressBtns[LEGEND]) && get_ptime(PressBtns[LEGEND]) < 200 && legend_check)  // Ash Quick Ult
                combo_run(ASH_QU_CMB);
            break;
        }
	// Mirage
		case mirage: {// Mirage Auto Decoy Mimic	
			if(event_press(PressBtns[LEGEND]) && legend_check)
				combo_run(MIRAGE_DM_CMB);
			break;
		}
	// Loba
		case loba: {// Easy Loba Bracelet
			if(get_ival(TACTICAL) && legend_check){   
    			combo_run(BRACELET_CMB1);
    		}
			if(event_release(TACTICAL) && legend_check){
    			combo_run(BRACELET_CMB2);
    		}
    		break;
    	}
	// Gibraltar
		case gibraltar: {
			if(get_ival(ADS) && get_brtime(ADS) <= 150){ // Gibraltar No Gun Shield
    			set_val(PressBtns[LEGEND],100);
     		}
			if(get_ival(PressBtns[LEGEND]) && legend_check){ // Gibraltar Tactical + Ultimate
				set_val(TACTICAL,100);
				set_val(PING,100);
			}
			if(event_release(PressBtns[LEGEND])){
				combo_run(GIBRALTAR_TU)
			}
			break;
     	}
     // Vantage
		case vantage: {
			if(get_ival(TACTICAL) && get_ptime(TACTICAL) > 500 && legend_check) // Vantage Animation Cancel & Heal
       			combo_run(VANTAGE_HJ_CMB);
			if(holster)
			{
				horizonX = TRUE;
				if(get_ival(PING) && get_ptime(PING) > 200 && not_selecting && !get_ival(ADS) && !get_ival(JUMP))
				{
					set_val(ADS,  100);
					//block_input(PING);
				}
				if(event_release(PING) && get_ptime(PING) <= 250)
