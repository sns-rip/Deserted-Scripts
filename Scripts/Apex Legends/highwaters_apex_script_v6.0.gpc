	if(no_aa && !ping && ((type == 1 && !get_ival(PressBtns[ACTIVATE])) || (type == 2 && get_ival(PressBtns[ACTIVATE])) || (type == 3 && keep[time_shot] > 0))) {	
		if(on == 0 && get_ival(FIRE))
