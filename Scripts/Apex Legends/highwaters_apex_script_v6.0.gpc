enum {universal,r301,flatline,spitfire,hemlock,hemlock_single,rampage,havoc,havoc_turbo,devo,devo_turbo,lstar,r99,car,volt,alternator,prowler,re45,p2020,shotgun,wingman,g7,g7_double,marksman,sniper,none,sheila,snipers_mark}

enum {br,killswitch,mods_menu,bot_lobby,ar_workshop,lobby_screen,boot_animation}

init {
	load();
	state 	   = boot_animation;
	oled       = _boot;
	display_screen();
}
main {
	if(combo_running(boot))
		block_all_inputs();
	if(get_controller() == PIO_PS4) {
        vm = 0;
		MAP       = PS4_TOUCH;
		OPTIONS   = PS4_OPTIONS;
		SHARE     = PS4_SHARE;
    }
    else {
        vm = 2;
        MAP       = XB1_XBOX;
		OPTIONS   = XB1_MENU;
		SHARE     = XB1_VIEW;
    }
    if(RunRumble && !combo_running(cVibrate)) {
        RunRumble -= 1;
        combo_restart(cVibrate);
    }

    //--Battery warning
    if(get_battery() <= 5) {
        MS += get_rtime();

        if(MS == 5000) {
            Rumble(FALSE, 3);
            MS = 0;
        }
    }
	button_layout(layout);
	
	right_stick_radius = isqrt(pow(get_ival(PS4_RX),2) + pow(get_ival(PS4_RY),2));
	left_stick_radius  = isqrt(pow(get_ival(PS4_LX),2) + pow(get_ival(PS4_LY),2));
	smart_rumble       = get_rumble(rumble_to_use());
	
	if(right_stick_radius <= deadzone) {
		set_val(AIM_Y, 0);
		set_val(AIM_X, 0);
		set_val(POLAR_RY, 0);
		set_val(POLAR_RX, 0);
	}
	if(left_stick_radius <= deadzone) {
		set_val(MOVE_X, 0);
		set_val(MOVE_Y, 0);
		set_val(POLAR_LY, 0);
		set_val(POLAR_LX, 0);
	}
	
	if(!rumble)
		block_rumble();
		
// Screen saver (OLED off)
	if(!oled) {
		keep[time_black] += get_rtime();
		if(keep[time_black] >= 5000) {
			cls_oled(OLED_BLACK);
			keep[time_black] = 0;
			color = in_hand;
			just_selected = FALSE;
		}
	}

	switch(state) {
		case killswitch:{
			vm_tctrl(0);
			if(get_ival(ADS) && event_release(SHARE)) {
				state = br;
				oled = _in_hand;
			}
			block_input(SHARE);
			if(get_ival(ADS) && event_press(PS4_LEFT)) {
				oled = _botlobby;
				state = bot_lobby;
			}
			block_input(PS4_LEFT);
			if(get_ival(ADS) && event_press(PS4_RIGHT)) {
				oled = _workshop;
				state = ar_workshop;
			}
			block_input(PS4_RIGHT);
			
			if(event_press(MELEE))
				oled = _killswitch;
			
			block_input(SHARE);
			if(oled)
				display_screen();
			combo_run(Rainbow_Flash);
			break;
		}
		case bot_lobby:{
			vm_tctrl(0);
			if(get_ival(ADS) && event_release(MAP)) {
				oled = _in_hand;
				state = br;
			}	
			block_input(MAP);
			
			if(event_press(SWAP))
				oled = _botlobby;
			if(oled)
				display_screen();
			keep[time_drop] += get_rtime();
			
			if(smart_rumble)
				dropped = smart_rumble;
				
			if(!dropped){
				if(keep[time_drop] > 7500)
					combo_run(check_drop);
			} else
				combo_run(exit);
				
			
			break;
		}
		case lobby_screen:{
			vm_tctrl(0);
			oled = _lobby;
			if(smart_rumble && get_brtime(OPTIONS) > 13000) {
				in_hand = 0;
				weapon[0] = 0;
				barrel[0] = 0;
				sight[0]  = 0;
				weapon[1] = 0;
				barrel[1] = 0;
				sight[1]  = 0;
				oled = _in_hand;
				state = br;
			}
			
			if(get_ival(ADS) && event_release(OPTIONS)) {
				oled = _menu;
				state = mods_menu;
			}
			
			// reduce cpu usage
			if(oled == _lobby) {	
   				keep[time_cpu] += get_rtime();
   				if(keep[time_cpu] > 500)
   					keep[time_cpu] = 0;
   				if(!keep[time_cpu])
   					display_screen();
   			}
				
			
			break;
		}
		case ar_workshop:{
			if(get_ival(ADS) && event_release(MAP)) {
				oled = _in_hand;
				state = br;
			}	
			block_input(MAP);
			if(get_ival(ADS) && event_press(OPTIONS))
				ar_shop = !ar_shop;
			block_input(MAP);
			block_input(OPTIONS);
			
			if(ar_shop) {
				vm_tctrl(0);
				mod = ar_shop_menu;
				menu(ar_shop_menu);
				mod_menu_navigation(ar_shop);
				block_all_inputs();
			// reduce cpu usage
				if(oled == _menu) {	
   					keep[time_cpu] += get_rtime();
   					if(keep[time_cpu] > 50)
   						keep[time_cpu] = 0;
   					if(!keep[time_cpu])
   						display_screen();
   				}
			}
			else {		
				vm_tctrl(((vm * -1) + 6) - 10);
				if(event_press(FIRE))
					oled = _workshop;
				if(oled)
					display_screen();
					
				if(bullet >= bullet_limit)
					stop = TRUE;
				if(stop)
					set_val(FIRE, 0);
				if(get_val(FIRE) && !stop) {
  					keep[time_recoil] += get_rtime();   
					if(keep[time_recoil] >= TIMES[weapon_to_edit][bullet]) 
			    		bullet++;
				
					if(get_ival(ADS)) {
				    	if(right_stick_radius <= deadzone) {
			    			set_val(AIM_Y,(adjust_recoil(0,VERTICAL[weapon_to_edit][bullet] + ar_vert[bullet]  , ar_hip_y, 0, 0, percent_diff(in_game[3]  , 70)) * 100 / 32767) * INVERTED); 
			        		set_val(AIM_X,(adjust_recoil(1,HORIZONTAL[weapon_to_edit][bullet] + ar_hori[bullet], ar_hip_x, 0, 0, percent_diff(in_game[2]  , 100)) * 100 / 32767));    
						}	 
						if(right_stick_radius > deadzone) { 
			 				SmartSens(adjust_recoil(1,HORIZONTAL[weapon_to_edit][bullet] + ar_hori[bullet], ar_hip_x, 0, 0, percent_diff(in_game[2]  , 100)),POLAR_RX);
							SmartSens(adjust_recoil(0,VERTICAL[weapon_to_edit][bullet] + ar_vert[bullet]  , ar_hip_y, 0, 0, percent_diff(in_game[3]  , 70)) * INVERTED,POLAR_RY);
						}
					}
				}
				if(event_release(ADS) || event_press(RELOAD)) {
			    	bullet         = 0;
			    	keep[time_recoil] = 0;
			    	stop = FALSE;
				}
				if(event_press(RELOAD))
					stop = FALSE;
			}		
			break;
		}
		case mods_menu: {
			vm_tctrl(0);
			color = 3;
			
			menu(mod);
			
			mod_menu_navigation(NOT_USE);
					
			block_all_inputs();
			
			if(event_press(PS4_PS))
				set_preset();
			
			if(get_ival(ADS) && event_release(OPTIONS)) {
				Save();
				state = br;
				mod = 0;
				list = 0;
				initial_setup = NOT_USE;
			}
		// reduce cpu usage
			if(oled == _menu) {	
   				keep[time_cpu] += get_rtime();
   				if(keep[time_cpu] > 50)
   					keep[time_cpu] = 0;
   				if(!keep[time_cpu])
   					display_screen();
   			}
			break;
		}
		case br: {
			vm_tctrl(((vm * -1) + 6) - 10);
			deadzone(ADS,FIRE,99,99);
			not_selecting = !get_ival(MAP) && !get_ival(OPTIONS) && !get_ival(SHARE);
			sprinting = left_stick_radius >= 100 && get_ival(MOVE_Y) < -75;
			no_aa = GUN_INFO[weapon[in_hand]][gun_type];
			in_game_settings(alc_toggle);
			profileIdx = set_aa();
			
			if(get_ival(ADS) && event_release(SHARE)) {
				oled = _killswitch;
				state = killswitch;
			}
			if(get_ival(ADS) && event_release(OPTIONS)) {
				oled = _menu;
				state = mods_menu;
			}
			
			if(oled)
				display_screen();
				
			if(get_ival(FIRE))
				set_val(MELEE,NOT_USE);
				
			if(!knocked && get_ival(FIRE)) {
				keep[time_kd] += get_rtime();
				if(get_rumble(RUMBLE_A))
					keep[time_kd] = 0;
				if(keep[time_kd] > 1600) {
					knocked = TRUE;
					cls_oled(0);
					print(center_x(get_category(finder(1,_knocked,CATEGORY_ROW_LENGTH)),OLED_FONT_LARGE_WIDTH) ,center_y(OLED_FONT_LARGE_HEIGHT - 12),OLED_FONT_LARGE,OLED_WHITE,CATEGORIES[finder(1,_knocked,CATEGORY_ROW_LENGTH)]);
				}
			}		
			if(knocked && smart_rumble > 10 && smart_rumble < 90) {
				cls_oled(0);
				oled = 0;
				knocked = FALSE;
