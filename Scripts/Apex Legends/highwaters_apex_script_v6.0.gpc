int adjustments,hor_side, new_ar, HS, at;
function adjust_recoil(axis, val, s, b, h, diff){	
	if(b && GUN_INFO[weapon[in_hand]][gun_type] == 2)
		at = 3 + axis;
	else
		at = axis;
	if(get_ival(ADS))
		hor_side = Barrel_Vals[b][at] + GUN_INFO[weapon[in_hand]][ads_v - axis];
	else
		hor_side = Barrel_Vals[b][at] + GUN_INFO[weapon[in_hand]][hip_v - axis];
	adjustments = Sight_Vals[s] + hor_side + diff;
	new_ar = val + (((val * adjustments) / 100));
	val = (new_ar * (response_curve * 100) / 1000);
	return val + h;
	//return val;
}

