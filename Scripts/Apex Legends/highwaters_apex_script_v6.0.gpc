define CATEGORY_ROW_LENGTH = 11;define LIST_ROW_LENGTH     = 14;define STRINGS_ROW_LENGTH  =  4;
define ar_list_start       =  1;define ar_list_end         = 11;define other_list_start    = 12;define other_list_end      = 24;
int list_on = 1,list_val[4];
enum {mod_blah,mod_type,mod_row,mod_val,arw_val}
function mod_menu_edit(f_mod, f_edit, f_limit, f_val, f_type, f_row, f_min, f_max, f_inc1, f_inc2) {  // antirecoil_start[profile_idx] = edit_val( 0 , antirecoil_start[profile_idx], 99  , 99  )
	//if(f_val > f_max)
	//	f_val = 0;
	if(finder(mod,second_in_list,LIST_ROW_LENGTH) == finder(f_mod,f_edit,LIST_ROW_LENGTH)) {
		//top[1] = finder(mod,second_in_list,LIST_ROW_LENGTH);
		top[mod_type] = f_type;
		top[mod_row] = f_row;
		top[mod_val] = f_val;
		top[arw_val] = ar_type_check(f_mod, second_in_list,f_val);
		list_val[2] = ar_type_check(f_mod, second_in_list,f_val);
	}
	if(finder(mod,third_in_list,LIST_ROW_LENGTH) == finder(f_mod,f_edit,LIST_ROW_LENGTH)) {
		//bottom[1] = finder(mod,third_in_list,LIST_ROW_LENGTH);
		bottom[mod_type] = f_type;
		bottom[mod_row] = f_row;
		bottom[mod_val] = f_val;
		bottom[arw_val] = ar_type_check(f_mod, third_in_list,f_val);
		list_val[3] = ar_type_check(f_mod, third_in_list,f_val);
	}
	if(finder(mod,first_in_list,LIST_ROW_LENGTH) == finder(f_mod,f_edit,LIST_ROW_LENGTH)) {
		//middle[1] = finder(mod,first_in_list,LIST_ROW_LENGTH);
		middle[mod_type] = f_type;
		middle[mod_row] = f_row;
		middle[mod_val] = f_val;
		middle[arw_val] = ar_type_check(f_mod, first_in_list,f_val);
		list_val[1] = ar_type_check(f_mod, first_in_list,f_val);
	}
	if(finder(mod,list,LIST_ROW_LENGTH) == finder(f_mod,f_edit,LIST_ROW_LENGTH)) {
		limit = f_limit;
		if(get_ival(ADS) || get_ival(PS4_L2)) {
			if(f_type == 1) {
			   if(event_press(PS4_UP))
				   f_val = 1;
			   else if(event_press(PS4_DOWN))
				   f_val = 0;
			}
			if(f_type == 2)
				f_val = scroll(f_val, f_min, f_max);
			if(f_type == 0) {
			// +1 or -1															 
				f_val    = value_change( f_val ,PS4_LEFT , inv(f_inc1), f_min, f_max ); // inv(f_inc1)
				f_val    = value_change( f_val ,PS4_RIGHT, f_inc1     , f_min, f_max ); // f_inc1
			// +10 or -10
				f_val    = value_change( f_val ,PS4_DOWN , inv(f_inc2), f_min, f_max ); // inv(f_inc2)
				f_val    = value_change( f_val ,PS4_UP   , f_inc2     , f_min, f_max ); // f_inc2
			}
		}
		if(f_type == 3) {
			if(singleButtonActivation() > 0)
				f_val = singleButtonActivation();
		}
	}
	oled = _menu;
	//list_val[finder(f_mod,f_edit,LIST_ROW_LENGTH)] = f_val;	
	return f_val;
}
int mod, list, limit, first_in_list, second_in_list, third_in_list;
function mod_menu_navigation(locked_category) {
	if(!get_ival(ADS) && !get_ival(PS4_L2)) {
		if(!locked_category) {
			if(event_release(PS4_RIGHT) && get_ptime(PS4_RIGHT) < 400) {
				mod += 1; 
				list = 0; 
				oled = _menu;
			}
			if(event_release(PS4_LEFT) && get_ptime(PS4_LEFT) < 400) {
				mod -= 1;
				list = 0; 
				oled = _menu;
			}
		//Cycle mods name
			if(!initial_setup) {
				if(mod > CATEGORY_ROW_LENGTH - (3 - (layout == 7)))  
					mod = 0;
				if(mod < 0)
					mod = CATEGORY_ROW_LENGTH - (3 - (layout == 7));
			} else {
				if(mod > CATEGORY_ROW_LENGTH - (3 - (layout == 7)))  
					mod = 0;
				if(mod < 8)
					mod = CATEGORY_ROW_LENGTH - (3 - (layout == 7));
			}
		}				
		if(!list)
			list_on = 1;
		if(event_release(PS4_UP) && get_ptime(PS4_UP) < 400 && list) {
			list -= 1;
			oled = _menu;
			if(list_on > 1)
				list_on -= 1;
		}	
		if(event_release(PS4_DOWN) && get_ptime(PS4_DOWN) < 400 && list < limit) {
			list += 1;
			oled = _menu;
			if(list_on < 3)
				list_on += 1;
		}
		if(list && (list != limit ^^ limit < 2)) {
			if(list_on == 1) {
				first_in_list = list;
				second_in_list = list + 1;
				third_in_list  = list + 2;
			}
			if(list_on == 2) {
				first_in_list = list - 1;
				second_in_list = list;
				third_in_list  = list + 1;
			}
			if(list_on == 3) {
				first_in_list = list - 2;
				second_in_list = list - 1;
				third_in_list = list;
			}
		}
		if(!list) {
			first_in_list = 0;
			second_in_list = 1;
			third_in_list = 2;
		}
		if(list == limit && limit > 2) {
			first_in_list = limit - 2;
			second_in_list = limit - 1;
			third_in_list = limit;
		}
	}
}
