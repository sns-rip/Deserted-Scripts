enum{home,taylor_menu,extra1_menu,extra2_menu,shot_menu,move_menu,wpn_menu,misc_menu,game_menu,custom_layout,ar_shop_menu}
function menu(_mod) {
	switch(_mod){
		case home: {
			legend                    	     = mod_menu_edit(0,     0,   1, legend                           ,   2,  _legend,   0,  23,    1,   1);
			LEGEND			             	 = mod_menu_edit(0,     1,   1, LEGEND                   		 ,   3,  _btn,      1,  20,    0,   0);
			break;
		}
		case taylor_menu: {
			if(taylor_on)
			{
				taylor_on                		= mod_menu_edit(1,     0,  13 - ((!aimAssist[taylor_on]) * 12), taylor_on                 	     ,   2,  _on,    0,   2,   1,   1);
				aimAssist[taylor_on]            = mod_menu_edit(1,     1,  13 - ((!aimAssist[taylor_on]) * 12), aimAssist[taylor_on]              ,   1,  0,    0,   1,   1,   1);
				if(aimAssist[taylor_on]) {	
					taylor_when[taylor_on]          = mod_menu_edit(1,     2,  13, taylor_when[taylor_on]            ,   2,  _when,    0,   4,   1,   1);
					taylorShapes[taylor_on]         = mod_menu_edit(1,     3,  13, taylorShapes[taylor_on]		     ,   2,  _shape,    0, 11,   1,   1);
	    			speedAngle[taylor_on]           = mod_menu_edit(1,     4,  13, speedAngle[taylor_on]             ,   0,  0,    0,  99,   1,  10);
           			radiusSize[taylor_on]           = mod_menu_edit(1,     5,  13, radiusSize[taylor_on]             ,   0,  0,    0,  99,   1,  10);
           			spiralChange[taylor_on]         = mod_menu_edit(1,     6,  13, spiralChange[taylor_on]           ,   1,  0,    0,   1,   1,   1);
           			boostOnFire[taylor_on]			= mod_menu_edit(1,     7,  13, boostOnFire[taylor_on]            ,   1,  0,    0,   1,   1,   1);
           			boostSpeed[taylor_on]			= mod_menu_edit(1,     8,  13, boostSpeed[taylor_on]             ,   0,  0,    0,  60,   1,  10);
           			boostMinRadius[taylor_on]		= mod_menu_edit(1,     9,  13, boostMinRadius[taylor_on]         ,   0,  0,    0,  30,   1,  10);
           			boostMaxRadius[taylor_on]		= mod_menu_edit(1,    10,  13, boostMaxRadius[taylor_on]         ,   0,  0,    0,  40,   1,  10);
					taylorTracking[taylor_on]       = mod_menu_edit(1,    11,  13, taylorTracking[taylor_on]         ,   2,  _track,    0,   3,   1,   1);
					trackingSize[taylor_on]         = mod_menu_edit(1,    12,  13, trackingSize[taylor_on]           ,   0,  0,    0,  20,   1,   1);
					trackingSpeed[taylor_on]        = mod_menu_edit(1,    13,  13, trackingSpeed[taylor_on]          ,   0,  0,    0,  40,   1,   1);
				}
			} else
				taylor_on                		 = mod_menu_edit(1,     0,   0, taylor_on                 		 ,   2,  _on,    0,   2,   1,   1);	
			break;
		}
		case extra1_menu: {
			if(other_on) {
				if(hstlrShapeShifter[other_on] && rotationalAA[other_on]) {
					other_on                 		 = mod_menu_edit(2,     0,  12, other_on                 		,   2,  _on,    0,   2,   1,   1);
					hstlrShapeShifter[other_on]		 = mod_menu_edit(2,     1,  12, hstlrShapeShifter[other_on]     ,   1,  0,    0,   1,   1,   1);
					maxThresholdOne[other_on]        = mod_menu_edit(2,     2,  12, maxThresholdOne[other_on]       ,   0,  0,    0, 100,   1,   10);
					maxThresholdTwo[other_on]	     = mod_menu_edit(2,     3,  12, maxThresholdTwo[other_on]       ,   0,  0,    0, 100,   1,   10);
					pointOneShape[other_on] 	     = mod_menu_edit(2,     4,  12, pointOneShape[other_on]		    ,   2,  _shape,    0, 11,   1,   1);
		    		pointTwoShape[other_on]	         = mod_menu_edit(2,     5,  12, pointTwoShape[other_on]		    ,   2,  _shape,    0, 11,   1,   1);
		    		pointThreeShape[other_on]        = mod_menu_edit(2,     6,  12, pointThreeShape[other_on]		,   2,  _shape,    0, 11,   1,   1);
		    		rotationalAA[other_on]      	 = mod_menu_edit(2,     7,  12, rotationalAA[other_on]         	,   2,  _type,   0,   2,   1,   1);
					rotation_when[other_on]          = mod_menu_edit(2,     8,  12, rotation_when[other_on]		    ,   2,  _when,    0,   4,   1,   1);
					rotationalSize [other_on]        = mod_menu_edit(2,     9,  12, rotationalSize [other_on]       ,   0,  0,    0, 100,   1,   10);
					rotationalSpeed[other_on]        = mod_menu_edit(2,    10,  12, rotationalSpeed[other_on]       ,   0,  0,    0, 100,   1,   10);
					rotateBoostToggle[other_on]      = mod_menu_edit(2,    11,  12, rotateBoostToggle[other_on]     ,   1,   0,   0,   1,   1,   1);
					rotationalBoostSize[other_on]    = mod_menu_edit(2,    12,  12, rotationalBoostSize[other_on]   ,   0,  0,    0, 100,   1,   10);
		    	} else if(!hstlrShapeShifter[other_on] && rotationalAA[other_on]){
		    		other_on                 		 = mod_menu_edit(2,     0,   7, other_on                 		,   2,  _on,    0,   2,   1,   1);
		    		hstlrShapeShifter[other_on]		 = mod_menu_edit(2,     1,   7, hstlrShapeShifter[other_on]     ,   1,  0,    0,   1,   1,   1);
		    		rotationalAA[other_on]      	 = mod_menu_edit(2,     2,   7, rotationalAA[other_on]         	,   2,  _type,   0,   2,   1,   1);
		    		rotation_when           		 = mod_menu_edit(2,     3,   7, rotation_when		            ,   2,  _when,    0,   4,   1,   1);
					rotationalSize [other_on]        = mod_menu_edit(2,     4,   7, rotationalSize [other_on]       ,   0,  0,    0, 100,   1,   10);
					rotationalSpeed[other_on]        = mod_menu_edit(2,     5,   7, rotationalSpeed[other_on]       ,   0,  0,    0, 100,   1,   10);
					rotateBoostToggle[other_on]      = mod_menu_edit(2,     6,   7, rotateBoostToggle[other_on]     ,   1,   0,   0,   1,   1,   1);
					rotationalBoostSize[other_on]    = mod_menu_edit(2,     7,   7, rotationalBoostSize[other_on]   ,   0,  0,    0, 100,   1,   10);
		    	} else if (hstlrShapeShifter[other_on] && !rotationalAA[other_on]) {
		    		other_on                 		 = mod_menu_edit(2,     0,   7, other_on                 		,   2,  _on,    0,   2,   1,   1);
		    		hstlrShapeShifter[other_on]		 = mod_menu_edit(2,     1,   7, hstlrShapeShifter[other_on]     ,   1,  0,    0,   1,   1,   1);
					maxThresholdOne[other_on]        = mod_menu_edit(2,     2,   7, maxThresholdOne[other_on]       ,   0,  0,    0, 100,   1,   10);
					maxThresholdTwo[other_on]	     = mod_menu_edit(2,     3,   7, maxThresholdTwo[other_on]       ,   0,  0,    0, 100,   1,   10);
					pointOneShape[other_on] 	     = mod_menu_edit(2,     4,   7, pointOneShape[other_on]		    ,   2,  _shape,    0, 11,   1,   1);
		    		pointTwoShape[other_on]	         = mod_menu_edit(2,     5,   7, pointTwoShape[other_on]		    ,   2,  _shape,    0, 11,   1,   1);
		    		pointThreeShape[other_on]        = mod_menu_edit(2,     6,   7, pointThreeShape[other_on]		,   2,  _shape,    0, 11,   1,   1);
		    		rotationalAA[other_on]      	 = mod_menu_edit(2,     7,   7, rotationalAA[other_on]         	,   2,  _type,   0,   2,   1,   1);
		    	} else if(!hstlrShapeShifter[other_on] && !rotationalAA[other_on]) {
		    		other_on                 		 = mod_menu_edit(2,     0,   2, other_on                 		,   2,  _on,    0,   2,   1,   1);
		    		hstlrShapeShifter[other_on]		 = mod_menu_edit(2,     1,   2, hstlrShapeShifter[other_on]     ,   1,  0,    0,   1,   1,   1);
		    		rotationalAA[other_on]      	 = mod_menu_edit(2,     2,   2, rotationalAA[other_on]         	,   2,  _type,   0,   2,   1,   1);
		    	}
			} else
				other_on                 		 = mod_menu_edit(2,     0,   0, other_on                 		 	,   2,  _on,    0,   2,   1,   1);
			break;
		}
		case extra2_menu: {
			if(headshot_assist_toggle && tdSteadyAim) {	
				headshot_assist_toggle    	     = mod_menu_edit(3,     0,   6 - ((tdSteadyAim == 2) * 2), headshot_assist_toggle   ,   1,   0,   0,   1,   1,   1);
				headshot_strength         	     = mod_menu_edit(3,     1,   6 - ((tdSteadyAim == 2) * 2), headshot_strength        ,   0,   0,   0,  99,   1,  10);
			   	headshot_hold_time        	     = mod_menu_edit(3,     2,   6 - ((tdSteadyAim == 2) * 2), headshot_hold_time       ,   0,   0,   0,1000,   1, 100);
        	   	tdSteadyAim     			 	 = mod_menu_edit(3,     3,   6 - ((tdSteadyAim == 2) * 2), tdSteadyAim        		,   2,   _toggle,   0,   2,   1,   1);
        	   	currentSAimValue    			 = mod_menu_edit(3,     4,   6 - ((tdSteadyAim == 2) * 2), currentSAimValue      	,   0,  0,    0, 100,   1,   10);
 				if(tdSteadyAim == 1) {
 					currentThreshold     			 = mod_menu_edit(3,     5,   6, currentThreshold      	 ,   0,  0,    0, 100,   1,   10);
					currentThresholdMax  			 = mod_menu_edit(3,     6,   6, currentThresholdMax      ,   0,  0,    0, 100,   1,   10);
				}
			} else if(!headshot_assist_toggle && tdSteadyAim) {	
				headshot_assist_toggle    	     = mod_menu_edit(3,     0,   4 - ((tdSteadyAim == 2) * 2), headshot_assist_toggle   ,   1,   0,   0,   1,   1,   1);
        	    tdSteadyAim     			 	 = mod_menu_edit(3,     1,   4 - ((tdSteadyAim == 2) * 2), tdSteadyAim        		,   2,   _toggle,   0,   2,   1,   1);
        	   	currentSAimValue    			 = mod_menu_edit(3,     2,   4 - ((tdSteadyAim == 2) * 2), currentSAimValue      	,   0,  0,    0, 100,   1,   10);
 				if(tdSteadyAim == 1) {	
 					currentThreshold     			 = mod_menu_edit(3,     3,   4, currentThreshold      	 ,   0,  0,    0, 100,   1,   10);
					currentThresholdMax  			 = mod_menu_edit(3,     4,   4, currentThresholdMax      ,   0,  0,    0, 100,   1,   10);
				}
			} else if(headshot_assist_toggle && !tdSteadyAim) {
