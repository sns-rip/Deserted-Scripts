		if(on == 1 && get_ival(FIRE) && get_ival(ADS))
			return TRUE;
		if(on == 2 && get_ival(FIRE) || get_ival(ADS))
			return TRUE;
		if(on == 3 && !get_ival(FIRE) && get_ival(ADS))
			return TRUE;
	}
	return FALSE;
}

