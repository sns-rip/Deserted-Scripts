function ar_type_check(m,r,f) {
	if(m == 10)
	{
		if(r == 7)
			to_return = adjust_recoil(0,VERTICAL[weapon_to_edit][_bullet], 0, 0, 0, percent_diff(in_game[3]  , 70)) + f;//VERTICAL[weapon_to_edit][_bullet];
		if(r == 8)
