//                COMBOS
//___________________________________________________________________________________________________
int _strafe,_strafe_index,_strafe_rand_1,_strafe_rand_2,sub;
combo strafe {
	if(_strafe_index == 0)
	{
		_strafe_rand_1 = strafe_time + strafe_time; //110
		_strafe_rand_2 = strafe_time + (strafe_time * 2);//210
	}
	else
	{
		_strafe_rand_1 = strafe_time + (strafe_time * 2);//210
		_strafe_rand_2 = strafe_time + strafe_time;//110
	}
	if(GUN_INFO[weapon[in_hand]][2] == 1 || GUN_INFO[weapon[in_hand]][2] == 2)
		sub = 23;
	else
		sub = 13;
	offset(MOVE_X, 100);
	if(right_stick_radius < 15)
		offset(POLAR_RX, adjust_recoil(1,polar(-sub), 0, 0, 0, percent_diff(in_game[2]  , 500)));
	_strafe = random((_strafe_rand_1 - strafe_time), (strafe_time + _strafe_rand_1));
	wait(_strafe); 
	wait(50);
	offset(MOVE_X, -100);
	if(right_stick_radius < 15)
		offset(POLAR_RX, adjust_recoil(1,polar(sub), 0, 0, 0, percent_diff(in_game[2]  , 500)));
	_strafe = random(( _strafe_rand_2 - strafe_time), (strafe_time + _strafe_rand_2));
	wait(_strafe); 
	wait(50);
	_strafe_index++;
	if(_strafe_index == 2)
		_strafe_index = 0;
}
combo crouch_plus {
	if(!get_ival(ADS))	
		set_val(CROUCH,100);
	wait(220);
	set_val(CROUCH,0);
	wait(_strafe);
}
combo CrouchShot {
	set_val(CROUCH,100);
	wait(crouch_time);
	set_val(CROUCH,0);
	wait(crouch_time);
}
int ping_time = 130;
combo auto_ping {	
	set_val(PING,100);
	wait(20);
	set_val(PING,0);
	if(get_ival(FIRE) && get_ptime(FIRE) > 120)
		ping_time = 80;
	if(get_ival(FIRE) && get_ptime(FIRE) < 120)
		ping_time = 130;
	wait(20);
	wait(ping_time);
}
combo heirloom_spin {	
