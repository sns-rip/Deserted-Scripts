init {
	Load();
	reset = TRUE;
	display_title = TRUE;
	just_selected = TRUE;
	
	if(get_console() != PIO_PS4)
		xb = 4;
	else 
		xb = 0;
	// PC + Xbox One + Xbox 360
    if(get_console() == PIO_XB1 || get_console() == PIO_XB360)
    {
        if(get_controller() == PIO_PS4)
             vm = 0;
        if(get_controller() == PIO_XB1)
            vm = 2;
    }
    // Xbox One Controller on anything else
    else if(get_controller() == PIO_XB1)
        vm = 2;
    // Anything else
    else
        vm = 4;
}


