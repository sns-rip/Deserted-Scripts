			cosAngle  = ((cos(DemonShapesInfo[getShapes][1] + getSinCos) * innerSpiral/DemonShapesInfo[getShapes][3] )/(23170) << 1) + trackingDeltaX;
    		sinAngle  = ((sin(DemonShapesInfo[getShapes][2] + getSinCos * 2) / 2 * outerSpiral/DemonShapesInfo[getShapes][4] )/(23170) << 1) + trackingDeltaY; 
       	}
        else { 
			cosAngle  = ((cos(DemonShapesInfo[getShapes][1] + getSinCos) * Rad/DemonShapesInfo[getShapes][3])/ (23170) << 1) + trackingDeltaX;
			sinAngle  = ((sin(DemonShapesInfo[getShapes][2] + getSinCos * 2) / 2 * Rad/DemonShapesInfo[getShapes][4])/ (23170) << 1) + trackingDeltaY;
		} 
	}
} 
function Helix(){ 
    increase = (increase % 32767);
    if(helixPosition == 0){ 
        x = increase; y = 32767 - increase;
       } 
    else if (helixPosition == 1){ 
        x = 32767 - increase; y = - increase; 
    }
    else if (helixPosition == 2){
        x = -increase; y = -32767 + increase; 
    }
    else if(helixPosition == 3) {  
        x = -32767 + increase; y = increase; 
    }
    helixPosition ++;
    if(helixPosition == 4){ 
        helixPosition = 0;
    }    
 }  
function atan2(int RY, int RX) {
    if (RX == 0) {
        if (RY > 0) {
            angle = 360;
        } else if (RY == 0) {
            angle = 0;
        } else {
            angle = -360;
        }
    } else if (RX > 0) {
        angle = correctAngle((RY * 10000) / RX);
    } else {
        angle = correctAngle((RY * 10000) / RX + 18000);
    }
    return angle;
}
  function round_fixed(int RX) {
    if (RX >= 0) {
        rounded = (RX + ANGLE_PRECISION / 2) / ANGLE_PRECISION;
    } else {
        rounded = (RX - ANGLE_PRECISION / 2) / ANGLE_PRECISION;
    }
    return rounded;
}
function correctAngle(angle){
  if (angle > 180) {
    angle -= 360;
  } else if (angle < -180) {
    angle += 360;
  }
  return angle;
}
function sin(atan2){
  return lerp_16(AkumuVal[(correctAngle(atan2(RY, RX) - 1))], AkumuVal[(correctAngle(atan2(RY, RX) + 1))], 0x8000);
}
function cos(atan2){
  return lerp_16(AkumuVal[((correctAngle(atan2(RY, RX) - 1) + 270) % 360)],  AkumuVal[((correctAngle(atan2(RY, RX)+ 1) + 270) % 360)], 0x8000);
}
function lerp_16(a, b, t) { return fix_to_int(lerp(int_to_fix(a), int_to_fix(b), t)); }
function int_to_fix(a) { return a * 0x10000; }
function fix_to_int(a) { if (a >= 0) return (a + 0x10000 >> 1) / 0x10000; return (a - 0x10000 >> 1) / 0x10000; }
function fix_mul(v1, v2) { return ((v1 >> 8) * (v2 >> 8)); }
function lerp(a, b, f) { return fix_mul((0x10000 - f), a) + fix_mul(f, b); }

function getPolar(AngleOrRadius, stick) {
  if (angleOrRadius) return (360 - get_polar(stick, POLAR_ANGLE)) % 360;
  return isqrt(~(pow(get_val(49 + stick), 2) + pow(get_val(50 + stick), 2))) + 3;}

