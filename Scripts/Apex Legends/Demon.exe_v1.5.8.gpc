int in_hand, to_set, weapon[3], barrel[2], sight[2];
function select(ih) {
//  weapon select
	to_set = ih;
// reset 										HOLD ADS and HOLD LEFT
	if(get_ptime(PS4_LEFT) > 300 && get_val(PS4_LEFT))
	{
		sight[to_set] = 0;
		barrel[to_set] = 0;
		weapon[to_set] = 0;
		color = 2;
		display_title = TRUE;
	}
	if(get_ival(PS4_LEFT))
		set_val(PS4_LEFT, 0);
// set AR                    -                    HOLD ADS and PRESS D-pad UP REPEAT to cycle through ARs
	if(event_press(PS4_UP))
	{	
		weapon[to_set] += 1;		
		if(weapon[to_set] <=  ar_list_start || weapon[to_set] > ar_list_end)
			weapon[to_set] = ar_list_start;
		reset = FALSE;
		display_select = 10;
	}
	if(get_ival(PS4_UP))
		set_val(PS4_UP, 0);
// set other                  -                   HOLD ADS and PRESS D-pad LEFT
	if(event_press(PS4_LEFT))
	{
		weapon[to_set] += 1;		
		if(weapon[to_set] <= other_list_start || weapon[to_set] > other_list_end)
			weapon[to_set] = other_list_start;
		reset = FALSE;
		display_select = 10;
	}
	if(get_ival(PS4_LEFT))
		set_val(PS4_LEFT, 0);
// set sight							HOLD ADS and PRESS RIGHT
	if(event_press(PS4_DOWN) && weapon[to_set] != 0)
	{
		sight[to_set] += 1;
		if(sight[to_set] > 2 || (GUN_INFO[weapon[to_set]][2] && GUN_INFO[weapon[to_set]][2] <= 2 && sight[to_set] > 1) || GUN_INFO[weapon[to_set]][2] == 4)
			sight[to_set] = 0;
		reset = FALSE;
		display_select = 2;
		color = sight[to_set] + 4;
	}
	if(get_ival(PS4_DOWN))
		set_val(PS4_DOWN, 0);  
// set barrel							HOLD ADS and PRESS DOWN
	if(event_press(PS4_RIGHT) && weapon[to_set] != 0)
	{
		barrel[to_set] += 1;
		if(barrel[to_set] > 3 || GUN_INFO[weapon[to_set]][2] > 1)
			barrel[to_set] = 0;
		reset = FALSE;
		display_select = 1;
		color = barrel[to_set] + 3;
	}
	if(get_ival(PS4_RIGHT))
		set_val(PS4_RIGHT, 0);
}
