         if(PEEP_Aimbot_mode)
         combo_run(PeepAimbot)
         if(abs(get_val(PS4_RX)) >= 15 || abs(get_val(PS4_RX)) >= 15 || abs(get_val(PS4_LY)) <= 15 || abs(get_val(PS4_LX)) <= 15)combo_stop(PeepAimbot)
          
