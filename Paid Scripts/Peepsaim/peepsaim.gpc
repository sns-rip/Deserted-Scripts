         combo_stop(PeepExpo2)
         
      combo_run(PeepExpo3)if(abs(get_val(PS4_RX)) > 17 || abs(get_val(PS4_RY)) > 17 || abs(get_val(PS4_LX)) > 16 || abs(get_val(PS4_LY)) > 16)
         combo_stop(PeepExpo3)    
         
         }}}
     
  
     
       
     
   



combo Confirm {
      
      set_val(Confirm,100);
      wait(20);
      set_val(Confirm,0);
      wait(100);
}



combo AimExpo{          
     
     set_val(10, (AimAssist_Strength)); wait(1)
     set_val(9, (AimAssist_Strength)); 
     set_val(10, -AimAssist_Strength ); wait(1)
     set_val(9, -AimAssist_Strength);
}

combo PeepAimbot {
set_val(PS4_RX,PEEP_Aimbot_Strength)// do no change
set_val(PS4_RY,PEEP_Aimbot_Strength)wait(1)// do no change
set_val(PS4_RX,-PEEP_Aimbot_Strength)// do no change
set_val(PS4_RY,-PEEP_Aimbot_Strength)wait(1)// do no change
}
int Peep1 = 8;
combo PeepExpo2 {
set_val(PS4_RX,Peep1)wait(1)
set_val(PS4_RY,Peep1)wait(1) 
set_val(PS4_LY,Peep1) 
set_val(PS4_LX,Peep1)}
combo PeepExpo3 {
set_val(PS4_RX,Peep1)wait(1)
set_val(PS4_RY,Peep1)wait(1) 
set_val(PS4_LY,Peep1)wait(1)  
set_val(PS4_LX,Peep1)}
int AR_Value;
function LegacyAR () {
    
    if(LegacyInvert) {
        set_Val(RY,AR_Value * -1);
    }
        else {    
            set_Val(RY,AR_Value);
        }
}



int AssistProfile = 1; 
function AimAssist (Strength,Step) {

	if (Power) {
		
		Ypower = Strength;
		Xpower = Strength;
		Increment = Step;
		Power = Off;
	}
	
	if (!power) {
	
		Xpower += Increment;
		Ypower -= Increment;
	
	} else {
		
		Xpower -= Increment;
		Ypower += Increment;
	}
	
	CountPower ++;
	
	if (CountPower >= (Strength / Increment)) {
		
		power = !power;
		CountPower = 0;
	}	
	
	
		}
	


function Press (Button) {

    if (get_ival(Button))
        return;
            set_val(Button, 100);
}

function set_Val(Input,Output) {

	set_val(Input,clamp(Output * (100 - abs(get_ival(Input))) / 100 + get_ival(Input),-100,100));
    return; 
}


define ADs      = PS4_ACCZ;
define Strafe   = PS4_LX;
define Walk     = PS4_LY;
int StrafeShot = TRUE;
int StrafeWait = 1;
main {
	if (StrafeShot) {
		if (get_ival(ADs) && abs(get_ival(Strafe)) <= 35 && abs(get_ival(Walk)) <= 35) {
			combo_run(StrafeShot);
					}
		else {
			combo_stop(StrafeShot);
					}
			}
	}
combo StrafeShot {
	set_val(Strafe,-100);
	wait(StrafeWait);
	set_val(Strafe,100);
	wait(StrafeWait);
	}

              
           
        
