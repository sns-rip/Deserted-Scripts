                                                                                                                                                  int stringLength;

                                                                                                                                                  function get_string_length(offset) { 
    stringLength = 0;
    do { 
        offset++;
        stringLength++;
    } while (duint8(offset));// 瞄准机器人
    return stringLength;
}



