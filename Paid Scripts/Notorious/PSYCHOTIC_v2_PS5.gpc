function reset_spvar() {

  pv_current_slot = SPVAR_1;
  pv_current_bit = 0;
  pv_current_value = 0;

}


function get_bit_count(f_val) {

  pv_temp = 0; 

  while(f_val) {
    pv_temp ++;  
    f_val = abs(f_val >> 1);  
  }

  return pv_temp;

}


function get_bit_count2(f_val1, f_val2) {

  pv_temp = max(get_bit_count(f_val1), get_bit_count(f_val2)); 

  if(is_signed2(f_val1, f_val2))  
    pv_temp ++;  

  return pv_temp;

}


function is_signed2(f_val1, f_val2) {

  return f_val1 < 0 || f_val2 < 0;

}


function make_sign(f_bits) {

  return 1 << clamp(f_bits - 1, 0, 31);

}


function make_full_mask(f_bits) {

  if(f_bits == 32)  
    return -1;

  return 0x7FFFFFFF >> (31 - f_bits);  

}


function make_sign_mask(f_bits) {

  return make_full_mask(f_bits - 1);

}


function pack_i(f_val, f_bits) {

  if(f_val < 0)
    return (abs(f_val) & make_sign_mask(f_bits)) | make_sign(f_bits);  

  return f_val & make_sign_mask(f_bits);  

}


function unpack_i(f_val, f_bits) {

  if(f_val & make_sign(f_bits)) 
    return 0 - (f_val & make_sign_mask(f_bits)); 

  return f_val & make_sign_mask(f_bits); 

}


function read_spvar_slot(f_slot) {

  return get_pvar(f_slot, 0x80000000, 0x7FFFFFFF, 0);

}


function save_spvar(f_val, f_min, f_max) {

  pv_bits = get_bit_count2(f_min, f_max);  

  f_val = clamp(f_val, f_min, f_max); 

  if(is_signed2(f_min, f_max))  
    f_val = pack_i(f_val, pv_bits);  

  f_val = f_val & make_full_mask(pv_bits); 

  if(pv_bits >= 32 - pv_current_bit) {  
    pv_current_value = pv_current_value | (f_val << pv_current_bit);  
    set_pvar(pv_current_slot, pv_current_value); 
    pv_current_slot ++;  
    pv_bits -= (32 - pv_current_bit);  
    f_val = f_val >> (32 - pv_current_bit); 
    pv_current_bit = 0;  
    pv_current_value = 0;  
  }

  pv_current_value = pv_current_value | (f_val << pv_current_bit);  

  pv_current_bit += pv_bits; 

  if(!pv_current_bit)
    pv_current_value = 0;  

  set_pvar(pv_current_slot, pv_current_value);  

}


function read_spvar(f_min, f_max) {

  pv_bits = get_bit_count2(f_min, f_max); 
  pv_current_value = (read_spvar_slot(pv_current_slot) >> pv_current_bit) & make_full_mask(pv_bits);  

  if(pv_bits >= 32 - pv_current_bit) {  
    pv_current_value = (pv_current_value & make_full_mask(32 - pv_current_bit)) | ((read_spvar_slot(pv_current_slot + 1) & make_full_mask(pv_bits - (32 - pv_current_bit))) << (32 - pv_current_bit));
   
  }

  pv_current_bit += pv_bits; 

  pv_current_value = pv_current_value & make_full_mask(pv_bits);  

  if(pv_current_bit >= 32) {
    pv_current_slot ++;  
    pv_current_bit -= 32; 
  }

  if(is_signed2(f_min, f_max))  
    pv_current_value = unpack_i(pv_current_value, pv_bits);  

  if(pv_current_value < f_min || pv_current_value > f_max) 
    return f_min;

 
  return pv_current_value;

}




