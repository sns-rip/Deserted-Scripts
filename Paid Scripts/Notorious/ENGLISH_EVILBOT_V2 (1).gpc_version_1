			CurRY = get_ival(aim_y);

			if (get_ival(Aim) || (get_ival(Aim) && get_ival(Fire))) {
				CurRX += get_polar_coordinate(NOTOROIOUS123499, sin_(TrackerAngle));
				CurRY += get_polar_coordinate(NOTOROIOUS123499, cos_(TrackerAngle));
			}
			else {
				CurRX += get_polar_coordinate(NOTOROIOUS123499, cos_(TrackerAngle));
				CurRY += get_polar_coordinate(NOTOROIOUS123499, sin_(TrackerAngle));
			}

			TrackerAngle = (TrackerAngle + NOTOROIOUS877778) % 360;
			set_val(aim_x, clamp(CurRX, -100, 100));
			set_val(aim_y, clamp(CurRY, -100, 100));

			
		}
	}

	
    
    if(HOTHACKR111122223333){
		if(	get_val(Fire) && get_ptime(Fire) <= 200 && !get_val(Aim)){
				combo_run(Ai_aimbot_ON);
				}
				if(combo_running(Ai_aimbot_ON) && (event_release(Aim) || !get_val(Fire) || event_press(BTN_TACTICAL) || event_press(BTN_LETHAL) || event_press(BTN_MELEE))){
			combo_stop(Ai_aimbot_ON)
			}
		}
	
		if(HOTHACKR6990009 ) {
				RY = NOTOROIOUS201009 * -1;
				if(get_ival(Aim) && get_ptime(Fire) < NOTOROIOUS20787) { 	
					offset(aim_y,RY);	
				}
			}
	
    if(HOTHACKR699000989 == 1){
    if (get_ival(Aim) || (get_ival(Aim) && get_ival(Fire)) || get_ival(Fire)){

    	centerX = get_ival(aim_x);
        centerY = get_ival(aim_y);

        offsetX = centerX + ((radius * sin_(angle)) / 1000);
        offsetY = centerY + ((radius * cos_(angle)) / 1000);
        set_val(aim_x, offsetX);
        set_val(aim_y, offsetY);

        if (isClockwise) {
            angle += angleStep * HOThACKR1232;
        } else {
            angle -= angleStep * HOThACKR1232;
        }

        petalCount++;

        if (petalCount >= HOThACKR129685788) {
            radius += (HOTKEY19 / 2);
            if (radius > (HOTKEY19)) {
                radius = HOTKEY19;
                spinsRemaining--;

                if (spinsRemaining <= 0) {
                    isClockwise = !isClockwise;
                    spinsRemaining = totalSpins;
                    angleStep = 359 / HOThACKR129685788;

                    if (!isClockwise) {
                        angleStep = -angleStep;
                    }
                }
            }
            petalCount = 0;
        }

        angle %= 360;
	}}
    if(HOTHACKR699000989 == 2){
