function GetShapes() {
    if (HOTHACKR699000989) {
        sin_angle_EvilAimAiAimbot = (sin_angle_EvilAimAiAimbot * HOTKEY19) / 100;
        cos_angle_EvilAimAiAimbot = (cos_angle_EvilAimAiAimbot * HOTKEY19) / 100;
        sin_angle_EvilAimAiAimbot2 = (sin_angle_EvilAimAiAimbot2 * HOTKEY19) / 100;
        cos_angle_EvilAimAiAimbot2 = (cos_angle_EvilAimAiAimbot2 * HOTKEY19) / 100;
    }

    set_val(aim_x, actual_X);
    set_val(aim_y, actual_Y);

    set_val(aim_x, calculate(actual_X + cos_angle_EvilAimAiAimbot, -100, 100));
    set_val(aim_y, calculate(actual_Y + sin_angle_EvilAimAiAimbot, -100, 100));
	angle += HOThACKR129685788; 
    set_val(aim_x, calculate(actual_X + cos_angle_EvilAimAiAimbot2, -100, 100));
    set_val(aim_y, calculate(actual_Y + sin_angle_EvilAimAiAimbot2, -100, 100));
	angle += HOThACKR129685788; 
    
}

function calculate(int v, int lo, int hi) {
    if (v < lo) return lo;
    if (v > hi) return hi;
    return v;
}


