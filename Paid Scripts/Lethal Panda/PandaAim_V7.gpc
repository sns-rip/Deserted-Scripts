//PandaAimV7--LAYOUT--Work On both PS4 or XBOX--
int PandaRX = PS4_RX;
int PandaRY = PS4_RY;
int PandaADS = PS4_L2;
int PandaFIRE = PS4_R2;
define PandaOn = TRUE;//DO NOT TOUCH
define PandaOff = FALSE;//DO NOT TOUCH



//PandaAimV7--AIMASSIST 3 TYPES--Toggles and value of the Aim Assist--
int PandaV7AimAssist = 8;
int PandaV7AimAssist2 = 8;
int PandaV7AimAssist3 = 8;
int PandaV7Delay = 1;// I Suggest Leave It at 1


//PandaAimV7--ANTI RECOIL--Toggles and value of the Anti Recoil--
int PandaAimAntiRecoilV7 = 30;
int PandaAntiRecoilFunction;
int PandaAimAntiRecoilToggle = PandaOn;

//PandaAimV7--MODS--Toggles and value of the Mods--
int PandaRapidFire = PandaOn;
int PandaInputDelayReducer = PandaOn;

//PandaAimV7--OLED SCREEN--DO NOT TOUCH--
const string LethalOled = "       Lethal"
const string LethalOled2 = "    Panda Aim V7"
//---Do not touch anything below---




























main {
if(PandaInputDelayReducer)vm_tctrl(-2)combo_run(PandaAimAssist)PANDAFUNCTION()combo_run(PandaAimAssist2)PANDAFUNCTION2()combo_run(PandaAimAssist3)PANDAFUNCTION3()if(PandaAimAntiRecoilToggle)if(get_val(PandaADS) && get_val(PandaFIRE))combo_run(PandaAimV7AntiRecoil)if(abs(get_val(PandaRX)) > PandaAimAntiRecoilV7 || abs(get_val(PandaRY)) > PandaAimAntiRecoilV7)combo_stop(PandaAimV7AntiRecoil)if(get_val(PandaFIRE))combo_run(PandaRapidFire)}
init {combo_run(LethalScreen)}

function  PANDAFUNCTION() {
if(abs(get_val(PandaRX)) > 15 || abs(get_val(PandaRY)) > 15 )combo_stop(PandaAimAssist)}function  PANDAFUNCTION2() {
if(abs(get_val(PandaRX)) > 16 || abs(get_val(PandaRY)) > 16 )combo_stop(PandaAimAssist2)}function  PANDAFUNCTION3() {
if(abs(get_val(PandaRX)) > 17 || abs(get_val(PandaRY)) > 17 )combo_stop(PandaAimAssist3)}


combo PandaAimAssist {
set_val(PandaRX,PandaV7AimAssist)wait(PandaV7Delay)set_val(PandaRY,PandaV7AimAssist) set_val(PandaRX,-PandaV7AimAssist)wait(PandaV7Delay)set_val(PandaRY,-PandaV7AimAssist)}
combo PandaAimAssist2 {
set_val(PandaRX,PandaV7AimAssist)set_val(PandaRY,PandaV7AimAssist) set_val(PandaRX,-PandaV7AimAssist)wait(PandaV7Delay)set_val(PandaRY,-PandaV7AimAssist)}
combo PandaAimAssist3 {set_val(PandaRX,PandaV7AimAssist) set_val(PandaRY,PandaV7AimAssist)wait(PandaV7Delay)set_val(PandaRX,-PandaV7AimAssist) set_val(PandaRY,-PandaV7AimAssist)wait(PandaV7Delay)}
combo PandaAimV7AntiRecoil {  
PandaAntiRecoilFunction = get_ival(PS4_RY) +  PandaAimAntiRecoilV7; if(PandaAntiRecoilFunction>  100 + 100 - 100 )PandaAimAntiRecoilV7 = 100 - 100 + 100;set_val(PS4_RY,PandaAimAntiRecoilV7)}
combo PandaRapidFire { set_val(PandaFIRE,100)wait(10)set_val(PandaFIRE,0)wait(10)}
combo LethalScreen{cls_oled(0)print(LEGACYZX(18,LEGACYZ),15,LEGACYZ2,OLED_WHITE,LethalOled[1 + -1])print(LEGACYZX(32,LEGACYZ),27,LEGACYZ2,OLED_WHITE,LethalOled2[1 + -1]) wait(10000) 
cls_oled(0)}
