combo DoGzDiminishSquare {

	DoGzDiminish(AimX,powVal);
	DoGzDiminish(AimY,inv(powVal));
	wait(ReducerTime);
	DoGzDiminish(AimX,powVal);
	DoGzDiminish(AimY,powVal);
	wait(ReducerTime);
	DoGzDiminish(AimX,inv(powVal));
	DoGzDiminish(AimY,powVal);
	wait(ReducerTime);
	DoGzDiminish(AimX,inv(powVal));
	DoGzDiminish(AimY,inv(powVal));
	wait(ReducerTime);
}

combo DoGzDiminishTriangle {

	DoGzDiminish(AimX,0);
	DoGzDiminish(AimY,inv(powVal));
	wait(ReducerTime);
	DoGzDiminish(AimX,powVal);
	DoGzDiminish(AimY,powVal);
	wait(ReducerTime);
	DoGzDiminish(AimX,inv(powVal));
	DoGzDiminish(AimY,powVal);
	wait(ReducerTime);
}

combo DoGzDiminishCross {

	DoGzDiminish(AimX,inv(powVal));
	DoGzDiminish(AimY,inv(powVal));
	wait(ReducerTime);
	DoGzDiminish(AimX,powVal);
	DoGzDiminish(AimY,powVal);
	wait(ReducerTime);
	DoGzDiminish(AimX,inv(powVal));
	DoGzDiminish(AimY,powVal);
	wait(ReducerTime);
	DoGzDiminish(AimY,inv(powVal));
	DoGzDiminish(AimX,powVal);
	wait(ReducerTime);
}

