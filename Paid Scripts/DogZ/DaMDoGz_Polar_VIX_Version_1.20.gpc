int ModSwitch;
int DoGzDiminish = On; 	// Aim Assist  [On - Off]
int AntiRecoil = On;    // Anti Recoil [On - Off]
int HeadAssist = On;    // Head Shot Assist [On - Off] (HipFire Only) 
int QuickPeek = On;     // Quick Peek (RB6 / PUBG ETC)
int ModDigit;
int AdsPower = 12;  	// Radius/Power On Ads Only [2 - 50]
int FirePower = 17;		// Radius/Power On Ads & Fire or Fire Only [2 - 50]
int FiringPower = 14;	// Radius/Power After Holding Fire For 1 Second [2 - 50]
int AdsSpeed = 20;		// Angle/Speed On Ads Only [1 - 165]
int FireSpeed = 50;     // Angle/Speed On Ads & Fire or Fire Only [1 - 165]
int FiringSpeed = 30;   // Angle/Speed After Holding Fire For 1 Second [1 - 165]
int FiringTime = 1000;  // Firing Time To Change Settings (Default Fire Time 1 Second) [1000ms = 1Second]
int recoilY1Start = 46; // Vertical Axis Start Power
int recoilY1End = 34;   // Vertical Recoil End Power
int recoilTY1 = 380;    // Vertical Time Travel Time [Time to Smoothly Move From Start Value To End Value] *10 (Default 380 * 10 = 3800 [3.8 Seconds])
int recoilX1Start = -3; // Horizontal Axis Start Power
int recoilX1End = 3;    // Horizontal Recoil End Power
int recoilTX1 = 200;    // Horizontal Time Travel Time [Time to Smoothly Move From Start Value To End Value] *10 (Default 200 * 10 = 2000 [2 Seconds])           
int HeadMax = 80;       // Max Value & Cuttoff Value
int HeadStep = 10;      // Increment to Max Amount
int PeekTime = 100;     // Peek Time (Length of Time You Peek For)                       

init {
	
	Load();
    combo_run(Boot);
    Colour = 255;
    assignValue(recoilY1Start,recoilY1End,recoilTY1,recoilX1Start,recoilX1End,recoilTX1);
}

main {
	
	if (DoGzDiminish) {
		
		if (get_ival(Ads) || get_ival(Fire)) {
			
			if (get_ival(Fire)) {
			
				Radius = FirePower;
				Angle = FireSpeed;
			
			 	if (get_ival(Fire) && get_ptime(Fire) > FiringTime) {
			
					Radius = FiringPower;
					Angle = FiringSpeed;
				} 
			} else {

				Radius = AdsPower;
				Angle = AdsSpeed;
			}

			DoGzDiminisher();
	    }      
	}
	
	if (HeadAssist) {
	    
	    if (!get_ival(Ads) && get_ival(Fire)) {
	    
	    	if (abs(get_ival(AimPY)) < HeadMax * 327) { 
	    		
	    		Head += HeadStep;
	    		combo_run(HeadAssist);
	    	}	
	    }
	    
	    if (!get_ival(Fire)) Head = 0;
	    	
	    if (Head >= HeadMax) combo_stop(HeadAssist);
	}
	
	if (QuickPeek) {
	
		if (get_ival(Ads)) {
		
			if (!combo_running(PeekRight) && event_release(PS4_L3) && get_ptime(PS4_L3) <= 170) {
				
				Shunt = PS4_L3;
				combo_run(PeekLeft);
								
			} else if (!combo_running(PeekLeft) && event_release(PS4_R3) && get_ptime(PS4_R3) <= 170) {
				
				Shunt = PS4_R3;
				combo_run(PeekRight);				
			}
			
			if (combo_running(PeekLeft) && event_release(PS4_R3)) combo_stop(PeekLeft);
			if (combo_running(PeekRight) && event_release(PS4_L3)) combo_stop(PeekRight);			
		}
	}
	
	if (AntiRecoil) {
	    
	    if (get_ival(Ads) && get_ival(Fire)) {
	    
	    	if (event_press(Ads) || event_press(Fire)) {
	    		
	    		assignValue(recoilY1Start,recoilY1End,recoilTY1,recoilX1Start,recoilX1End,recoilTX1);	
	    	}
	    	
	    	recoil();
	    	powerY(recoilYStart,recoilYEnd,recoilTY);
	    	powerX(recoilXStart,recoilXEnd,recoilTX);
	    }
	}	    
	
    if (!MainMenu && get_ival(MenuEnterHold) && event_press(MenuEnterPress)) {

        Shunt = MenuEnterPress;
        MenuToggle(On,Off,On,On,On);
        combo_run(Rumble);
        combo_stop(Saved);
    }

    if (MainMenu) {

        combo_stop(Boot);
        MenuTimeout += get_rtime();

        if (get_ival(Up) || get_ival(Down) || get_ival(Left) || get_ival(Right) || event_press(ChangeMenuPress)) MenuTimeout = Off;

            if (MenuTimeout >= TimeoutLimitM || event_press(MenuExitPress)) {
            
