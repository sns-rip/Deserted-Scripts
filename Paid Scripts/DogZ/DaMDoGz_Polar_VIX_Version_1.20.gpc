define Fire  = PS4_R2;   // Fire Button In Game  [Default = R2/RT]
define Ads   = PS4_L2;   // Ads Button In Game   [Default = L2/LT]
define AimPX = POLAR_RX; // Aim Stick Horizontal [Default = RIGHT ANALOG STICK] (POLAR_RX or POLAR_LX)
define AimPY = POLAR_RY; // Aim Stick Vertical   [Default = RIGHT ANALOG STICK] (POLAR_RY or POLAR_LY)
define AimAX = ANALOG_RX;// Aim Stick Horizontal [Default = RIGHT ANALOG STICK] (ANALOG_RX or ANALOG_LX)
define AimAY = ANALOG_RY;// Aim Stick Vertical   [Default = RIGHT ANALOG STICK] (ANALOG_RY or ANALOG_LY)

/*
‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó
