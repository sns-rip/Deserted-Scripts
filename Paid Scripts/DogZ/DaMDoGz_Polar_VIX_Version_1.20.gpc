function LedFlow () {
    
    Rgb += get_rtime();
    
    if (Rgb >= LedSpeed) {
        
        set_rgb(Colour / LedBrightness,Colour[1] / LedBrightness,Colour[2] / LedBrightness);
        Colour[LedDec] -= 1; 
        Colour[LedInc] += 1;

        if (!Colour[LedDec]) { 
            
            LedInc = (LedInc + 1) % 3; 
            LedDec = (LedDec + 1) % 3; 
        }
        Rgb = 0;    
    }
}

function powerY(startVal,endVal,totalTime) {
    	   
    countY++;
    stepsY = abs(endVal - startVal);
    applyY = totalTime / stepsY;

    if (applyY < 1) applyY = 1;

    if (countY >= applyY) {
        
        if (startVal < endVal) powerY = clamp(powerY++,startVal,endVal);
        else if (startVal > endVal) powerY = clamp(powerY--,endVal,startVal);
        
        countY = 0;
    }
}

function powerX(startVal,endVal,totalTime) {
    	   
    countX++;
    stepsX = abs(endVal - startVal);
    applyX = totalTime / stepsX;

    if (applyX < 1) applyX = 1;

    if (countX >= applyX) {
        
        if (startVal < endVal) powerX = clamp(powerX + 1,startVal,endVal);
        else if (startVal > endVal) powerX = clamp(powerX - 1,endVal,startVal);
        
        countX = 0;
    }
}

function assignValue(startY,endY,timeY,startX,endX,timeX) {
	
	recoilYStart = startY;
	recoilYEnd = endY;
	recoilTY = timeY;
	recoilXStart = startX;
	recoilXEnd = endX;
	recoilTX =timeX;
	powerY = recoilYStart;
	powerX = recoilXStart;
}

function recoil() {	
	
	if (AxisInverted) recoilApply(AimAY,inv(powerY));
	else recoilApply(AimAY,powerY);
	recoilApply(AimAX,powerX);
}

function recoilApply(s,v) {
	
	set_val(s,clamp((v * 10000/100) * (10000 - abs(get_val(s))) / 10000 + get_val(s), -10000, 10000));
}

function DoGzDiminisher() {
	
	aimAssistRadius = ((Radius + 2) >> 1 * (32767 + (Radius + 2) >> 1 - isqrt(pow(abs(get_val(AimPX)), 2) + pow(abs(get_val(AimPY)), 2)))) / 32767;
	angle = (angle + Angle) % 360;
	cosAngle = ((cos(angle) * (aimAssistRadius * 327))/(23170) << 1);
	sinAngle = ((sin(angle) * (aimAssistRadius * 327))/(23170) << 1);
	set_val(AimPX,clamp(get_val(AimPX) + cosAngle,-32767,32767));
	set_val(AimPY,clamp(get_val(AimPY) + sinAngle,-32767,32767));
}

function correctAngle(angle){
  
  	if (angle < 0) angle = 360 + (angle % 360);
  	angle = (angle + 90) % 360;
  	return angle;
}

function sin(angle){
  
  	return lerp_16(pollarArr[(correctAngle(angle - 1))], pollarArr[(correctAngle(angle + 1))], 0x8000);
}

function cos(angle){
  
  	return lerp_16(pollarArr[((correctAngle(angle - 1) + 90) % 360)],  pollarArr[((correctAngle(angle + 1) + 90) % 360)], 0x8000);
}

function lerp_16(a, b, t) { 
	
	return fix_to_int(lerp(int_to_fix(a), int_to_fix(b), t)); 
}

function int_to_fix(a) { 

	return a * 0x10000; 
}

function fix_to_int(a) { 

	if (a >= 0) return (a + 0x10000 >> 1) / 0x10000; return (a - 0x10000 >> 1) / 0x10000; 
}

function fix_mul(v1, v2) { 

	return ((v1 >> 8) * (v2 >> 8)); 
}

function lerp(a, b, f) { 

	return fix_mul((0x10000 - f), a) + fix_mul(f, b); 
}

