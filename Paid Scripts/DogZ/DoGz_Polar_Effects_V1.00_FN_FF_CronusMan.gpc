(                   Functions                        ) 
 `-~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-* 
*/ 
function EditAssist () {

    EditT += get_rtime();
    
    if (get_ival(EditAssist)) {
    	
    	if (EditT >= 30) Press(Select);
    }
}

function Press (Button) {

    if (get_ival(Button)) return;
    
    set_val(Button, 100);
}

function SixthSenseRecoil() {
   
	if (Start) {
		
    	vTime = Off;
    	MoveY = Off;
    	MovementY = Off;
    	vP = 5;
    	vT = 11;
	    Start = Off;
	}
	
	if (AntiRecoilMode == RecoilSet) {
	
	    YAxis = ModDigit[vP];
    
	    if (vTime >= ModDigit[vT]) {
	    
	        if (get_ival(Ads) && get_ival(Fire)) {
	        	
	        	if (vT == 16 && vTime >= ModDigit[vT]) {
	        		vP = 4;
	        		vT = 10;
	        	}
	        }
	        
	        vTime = Off;
	        vP = clamp(vP++,5,10);
	        vT = clamp(vT++,11,16);
	    }		
	}
	
	if (AntiRecoilMode == RecoilProgression) {
	
		if (!MovementY) {
	
			if (!MoveY)	
				
				YAxis = ModDigit[vP];
	
			if (vTime >= ModDigit[vT]) {
				
				MoveY = On;
		
			if (ModDigit[vP] > ModDigit[vP + 1]) 
				
				YAxis --;
			
			if (ModDigit[vP] < ModDigit[vP + 1]) 
				
				YAxis ++;
				vTime = Off;
		}
		
		if (YAxis == ModDigit[vP + 1]) {
			
			MoveY = Off;
			vP = clamp(vP++,5,10);
	        vT = clamp(vT++,11,16);
		}
			else if (vP == 10) {
				
				MovementY = On;
			}
		}
			    	
	    YAxis = YAxis;
	}
    
    vTime += get_rtime();
}

function Type(typeDraw,typeAngle,typeX,typeY,typeRatio,Inverter,Style,OffsetX,OffsetY) {

	Type[0] = typeDraw;
	Type[1] = typeAngle;
	Type[2] = typeX;
	Type[3] = typeY;
	Type[4] = Style;
	
	if (typeRatio == 1) angle += AssistSpeed % High; 
	if (typeRatio == 2) angle += AssistSpeed;
	if (typeRatio == 3) angle = (angle + AssistSpeed) % High;
	
	if (Inverter == 1) {
	    
	    if (angle >= High) Switcher = !Switcher;
	    
	    if (Switcher) { 
	    	 		
	    	 xAdjust = 2; yAdjust = 1;
	    	 	
	    } else { 
	    	  		
	    	 xAdjust = 1; yAdjust = 2;
	    }
	}
	
	if (Inverter == 2) {
	
		if (angle >= High) Switcher = 0;
		if (angle <= Center) Switcher = 1;
		
		if (Switcher) {
		
			angle += AssistSpeed % High;
		
		} else {
			
			angle -= AssistSpeed % Center;
		}
	}
	
	if (Inverter == 3) {
		
		if (angle >= Center) Switcher = 0;
		if (angle <= Low) Switcher = 1;
		
		if (Switcher) {
		
			angle += AssistSpeed % Center;
		
		} else { 
		
			angle -= AssistSpeed % Low;
		}
	}
	
	if (Inverter == 4) {
		
		if (angle >= Center) Switcher =!Switcher;
		if (angle <= Low) Switcher = 1;
		
		xAdjust = 1; yAdjust = 1;
		 	
		if (Switcher) {
		 	
			yAdjust = 80; xAdjust = 1; 
