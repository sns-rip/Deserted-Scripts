		}
	}
	
	if (Inverter == 5) {	
		
		if (angle >= Center) Switcher = !Switcher;
		if (angle <= Low) Switcher =!Switcher; 
		
		xAdjust = 1; yAdjust = 1;
		
		if (Switcher) {
			
			yAdjust = 80; xAdjust = 1; 
			angle += AssistSpeed % Center;
		
		} else {
			
			angle -= AssistSpeed % Low;
		}
	}
	
	if (Inverter == 6) {
		
		if (angle >= High) Switcher = !Switcher;
		if (angle <= Center) Switcher = 1;
		
		xAdjust = 1; yAdjust = 1;
		
		if (Switcher) {
			
			yAdjust = 80; xAdjust = 1; 
			angle += AssistSpeed % High;
		 
		} else {
			
			angle -= AssistSpeed % Center;	
		}
	}
	
	if (Inverter == 7) {
	
		if (Switcher > 0) {
				
			yVal = 12; xVal = -12;
		}
			
		if (Switcher > 2) {
			
			yVal = 12; xVal = 12;
		}
		
		if (Switcher > 3) {
			
			yVal = -12; xVal = 12;
		}
		
		if (Switcher > 4) {
			
			yVal = -12; xVal = -12;
		}
		
		Switcher++;
		
		if (Switcher >= 5) Switcher = 0;
	}

	if (Inverter == 8) {
		
		if (Switcher < 120) {
			
			yVal += 1; xVal += 3;
		}
		
		Switcher++;
		
		if (Switcher >= 120) Switcher = 0;
	}
	
	if (Inverter == 9) {
	
		if (Switcher < 5) {
			yVal += 45; xVal += 30;
		}
		
		Switcher++;
		
		if (Switcher >= 5) Switcher = 0;
	}
	
	if (Inverter == 10 && Pattern == 2) {
		
		Switcher = !Switcher;	  	
			
		if (Switcher) {
		  
		  	cosAngle  = ((cos(angle + OffsetX) / xAdjust * (aimAssistRadius * 327))/(23170) << 1);
		  	sinAngle  = ((sin(angle + OffsetY) / yAdjust * (aimAssistRadius * 327))/(23170) << 1);
			
		} else { 
			
			cosAngle  = ((cos(angle + OffsetY) / xAdjust * (aimAssistRadius * 327))/(23170) << 1);
		  	sinAngle  = ((sin(angle + OffsetX) / yAdjust * (aimAssistRadius * 327))/(23170) << 1);
		} 
	}
	
	if (Inverter == 11 && Pattern == 3) {
		
		Switcher = !Switcher;	  	
			
		if (Switcher) {
		  
		  	cosAngle  = ((cos(angle + OffsetX) / xAdjust * (aimAssistRadius * 327))/(23170) << 1);
		  	sinAngle  = ((sin(angle + OffsetX) / yAdjust * (aimAssistRadius * 327))/(23170) << 1);
			
		} else { 
			
			cosAngle  = ((cos(angle + OffsetY) / xAdjust * (aimAssistRadius * 327))/(23170) << 1);
		  	sinAngle  = ((sin(angle + OffsetX) / yAdjust * (aimAssistRadius * 327))/(23170) << 1);
		} 
	}
	
	if (Inverter == 12 && Pattern == 4) {
		
		Switcher++;	  	
			
		if (Switcher == 1) {
		  
		  	cosAngle  = ((cos(angle  + 265) / xAdjust * (aimAssistRadius * 327))/(23170) << 1);
		  	sinAngle  = ((sin(angle  + 90) / yAdjust * (aimAssistRadius * 327))/(23170) << 1);
		}
		
