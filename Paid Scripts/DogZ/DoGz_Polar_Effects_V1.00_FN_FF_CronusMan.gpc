		} 
		
		if (Switcher == 4) { 
			
			cosAngle  = ((cos(angle + 290) / xAdjust * (aimAssistRadius * 327))/(23170) << 1);
		  	sinAngle  = ((sin(angle + 285) / yAdjust * (aimAssistRadius * 327))/(23170) << 1);
		} 
		
		if (Switcher >= 5) Switcher = 0;
	}
}

function correctAngle(angle){
  
  if (angle < 0) angle = 360 + (angle % 360);
  angle = (angle + Angle) % 360;
  
  return angle;
}

function sin(angle){
  
  if (Type == 1)
  return lerp_16(CircleArray[(correctAngle(angle - 1))], CircleArray[(correctAngle(angle + 1))], 0x8000);
  
  else if (Type == 2)
  return lerp_16(DiamondArray[(correctAngle(angle - 1))], DiamondArray[(correctAngle(angle + 1))], 0x8000);
  
  else if (Type == 3)
  return lerp_16(AngleArray[(correctAngle(angle - 1))] * 257, AngleArray[(correctAngle(angle + 1))] * 257, 0x8000);
  
  return 0;
}

function cos(angle){
  
  if (Type == 1)
  return lerp_16(CircleArray[((correctAngle(angle - 1) + Angle) % 360)],  CircleArray[((correctAngle(angle + 1) + Angle) % 360)], 0x8000);
  
  else if (Type == 2)
  return lerp_16(DiamondArray[((correctAngle(angle - 1) + Angle) % 360)],  DiamondArray[((correctAngle(angle + 1) + Angle) % 360)], 0x8000);
  
  else if (Type == 3)
  return lerp_16(AngleArray[((correctAngle(angle - 1) + Angle) % 360)] * 257,  AngleArray[((correctAngle(angle + 1) + Angle) % 360)] * 257, 0x8000);
  
  return 0;
}

function lerp_16(a,b,t) { 
	
	return fix_to_int(lerp(int_to_fix(a),int_to_fix(b),t)); 
}

function int_to_fix(a) { 
	
	return a * 0x10000; 
}

function fix_to_int(a) {

	if (a >= 0) return (a + 0x10000 >> 1) / 0x10000; return (a - 0x10000 >> 1) / 0x10000; 
}

function fix_mul(v1,v2) { 
	
	return ((v1 >> 8) * (v2 >> 8)); 
}

function lerp(a,b,f) { 

	return fix_mul((0x10000 - f),a) + fix_mul(f,b); 
}

